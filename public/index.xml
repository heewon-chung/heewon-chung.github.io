      <description>&lt;!DOCTYPE html&gt;&#xA;&lt;html lang=&#34;ko&#34;&gt;&#xA;&lt;head&gt;&#xA;    &lt;meta charset=&#34;UTF-8&#34;&gt;&#xA;    &lt;title&gt;About&lt;/title&gt;&#xA;    &lt;meta name=&#34;viewport&#34; content=&#34;width=device-width, initial-scale=1.0&#34;&gt;&#xA;    &lt;style&gt;&#xA;        html, body {&#xA;            background: #22272e !important;&#xA;            height: 100%;&#xA;            min-height: 100%;&#xA;        }&#xA;        body {&#xA;            font-family: &#39;Noto Sans KR&#39;, &#39;Segoe UI&#39;, Arial, sans-serif;&#xA;            color: #e0e0e0;&#xA;            margin: 0;&#xA;            padding: 0;&#xA;        }&#xA;        .container {&#xA;            max-width: 1000px;&#xA;            margin: 0 auto;&#xA;            padding: 0 4px 24px 4px;&#xA;        }&#xA;        h1 {&#xA;            color: #fff;&#xA;            margin-bottom: 1.5rem;&#xA;            font-size: 1.8rem;&#xA;            letter-spacing: -1px;&#xA;        }&#xA;        .section-title {&#xA;            color: #bdd3de; /* Slate Blue for section titles */&#xA;            font-size: 1.2rem;&#xA;            font-weight: bold;&#xA;            margin-top: 2.5rem;&#xA;            margin-bottom: 1.2rem;&#xA;        }&#xA;        .card-list {&#xA;            list-style: none;&#xA;            padding: 0;&#xA;            margin: 0;&#xA;        }&#xA;        .card {&#xA;            background: #23272b;&#xA;            border-radius: 8px;&#xA;            box-shadow: 0 2px 8px rgba(0,0,0,0.08);&#xA;            padding: 16px 14px 12px 14px;&#xA;            margin-bottom: 14px;&#xA;            transition: box-shadow 0.2s, background 0.3s;&#xA;        }&#xA;        .card-title {&#xA;            color: #4da6ff;&#xA;            font-size: 1.05rem;&#xA;            font-weight: bold;&#xA;            margin-bottom: 0.2rem;&#xA;        }&#xA;        .card-meta {&#xA;            /* 4Î≤à ÏïÑÏù¥ÎîîÏñ¥ Ï†ÅÏö©: ÏÉâÏÉÅ ÎØ∏ÏÑ∏ Ï°∞Ï†ï */&#xA;            color: #999;&#xA;            font-size: 0.92rem;&#xA;            margin-bottom: 0.2rem;&#xA;        }&#xA;        .card-desc {&#xA;            color: #e0e0e0;&#xA;            font-size: 0.98rem;&#xA;            margin-bottom: 0.1rem;&#xA;        }&#xA;        .linkedin-btn {&#xA;            padding: 0;&#xA;            background: none;&#xA;            border: none;&#xA;            box-shadow: none;&#xA;            border-radius: 0;&#xA;            display: inline-block;&#xA;        }&#xA;        .linkedin-btn:hover {&#xA;            background: #7fdde9;&#xA;            color: #23272b;&#xA;        }&#xA;        .timeline {&#xA;            display: flex;&#xA;            flex-direction: column;&#xA;            position: relative;&#xA;            margin: 2.5rem 0 2rem 0;&#xA;        }&#xA;        .timeline::before {&#xA;            content: &#39;&#39;;&#xA;            position: absolute;&#xA;            left: 130px;&#xA;            top: 0;&#xA;            width: 2px;&#xA;            height: var(--timeline-line-height, 100%);&#xA;            background: #4da6ff;&#xA;        }&#xA;        .timeline-item {&#xA;            position: relative; /* For icon positioning */&#xA;            display: flex;&#xA;            align-items: flex-start;&#xA;            margin-left: -50px;&#xA;            margin-bottom: 2.2rem;&#xA;            opacity: 0;&#xA;            transform: translateY(30px);&#xA;            transition: opacity 0.7s cubic-bezier(.4,0,.2,1), transform 0.7s cubic-bezier(.4,0,.2,1);&#xA;            min-height: 60px;&#xA;        }&#xA;        .timeline-item.visible {&#xA;            opacity: 0.5; /* Fade out past jobs */&#xA;            transform: none;&#xA;        }&#xA;        .timeline-item.visible.current,&#xA;        .timeline-item.visible:hover {&#xA;            opacity: 1; /* Highlight current job and hovered items */&#xA;        }&#xA;&#xA;        /* Education Section Styling */&#xA;        .education-item {&#xA;            display: flex;&#xA;            align-items: flex-start;&#xA;            margin-bottom: 1.5rem; /* Spacing between items */&#xA;            margin-left: -50px; /* Align with career timeline items */&#xA;        }&#xA;&#xA;        .education-date {&#xA;            flex: 0 0 70px; /* Fixed width for the year */&#xA;            padding-top: 0;&#xA;            line-height: 1.5;&#xA;            font-size: 1.08rem;&#xA;            color: #c0c8d8;&#xA;        }&#xA;&#xA;        .education-list .timeline-title {&#xA;            text-indent: 0;&#xA;        }&#xA;        .education-list .timeline-title::before {&#xA;            /* content: &#39;üéì&#39;;&#xA;            display: inline-block;&#xA;            width: 28px;&#xA;            font-size: 1.1rem;&#xA;            margin-right: 4px; */&#xA;        }&#xA;        .education-list .education-item.visible {&#xA;            opacity: 1 !important;&#xA;        }&#xA;        /* ÌÉÄÏûÑÎùºÏù∏ ÌïôÏúÑ/ÏßÅÏû•Î™Ö Ï†ïÎ†¨ ÌÜµÏùº */&#xA;        .timeline, .education-list {&#xA;            margin-left: 100px;&#xA;        }&#xA;        .timeline-date, .education-list .timeline-date {&#xA;            width: 180px;&#xA;            min-width: 180px;&#xA;            margin-right: 16px;&#xA;            text-align: left;&#xA;            color: #c0c8d8;&#xA;        }&#xA;        .timeline-content, .education-list .timeline-content {&#xA;            margin-left: 10px;&#xA;            min-width: 300px;&#xA;            max-width: 1100px;&#xA;        }&#xA;        .timeline-date-start {&#xA;            display: inline-block;&#xA;            min-width: 60px;&#xA;            text-align: left;&#xA;        }&#xA;        .timeline-date-dash {&#xA;            margin: 0 4px;&#xA;        }&#xA;        .timeline-date-end {&#xA;            min-width: 60px;&#xA;            text-align: left;&#xA;            margin-right: 0px;&#xA;        }&#xA;        .timeline-title {&#xA;            color: #4da6ff;&#xA;            font-size: 1.08rem;&#xA;            font-weight: bold;&#xA;            margin-bottom: 0.1rem;&#xA;            line-height: 1.5;&#xA;            white-space: nowrap;&#xA;            overflow: hidden;&#xA;            text-overflow: ellipsis;&#xA;        }&#xA;        .timeline-desc {&#xA;            color: #e0e0e0;&#xA;            font-size: 0.98rem;&#xA;            line-height: 1.5;&#xA;        }&#xA;        .timeline-desc-list {&#xA;            list-style: none;&#xA;            padding: 0;&#xA;            margin: 0;&#xA;        }&#xA;&#xA;        .promotion-path {&#xA;            position: relative;&#xA;            padding-left: 25px;&#xA;            margin-top: 10px;&#xA;        }&#xA;&#xA;        .promotion-path .promotion-step {&#xA;            position: relative;&#xA;        }&#xA;        .promotion-path .promotion-step:not(:last-child) {&#xA;            padding-bottom: 20px;&#xA;        }&#xA;        .promotion-path .promotion-step::before {&#xA;            content: &#39;&#39;;&#xA;            position: absolute;&#xA;            left: -20px; /* Adjusted for smaller size */&#xA;            top: 7px;   /* Adjusted for smaller size */&#xA;            width: 5px;   /* Smaller circle */&#xA;            height: 5px;  /* Smaller circle */&#xA;            border-radius: 50%;&#xA;            background-color: #22272e;&#xA;            border: 2px solid #4da6ff;&#xA;            z-index: 1;&#xA;        }&#xA;&#xA;        .promotion-step .role {&#xA;            display: block;&#xA;            font-weight: 500;&#xA;            color: #e0e0e0;&#xA;            font-size: 0.98rem;&#xA;        }&#xA;        .promotion-step .period {&#xA;            display: block;&#xA;            font-size: 0.85rem;&#xA;            color: #b0b0b0;&#xA;            margin-top: 4px;&#xA;        }&#xA;&#xA;        /* ÌïôÏúÑÎ™Ö/ÌïôÍµêÎ™Ö Î∂ÑÎ¶¨ Ïä§ÌÉÄÏùº */&#xA;        .degree-label {&#xA;            color: #4da6ff;&#xA;            font-weight: bold;&#xA;            font-size: 1.08rem;&#xA;        }&#xA;        .education-list .degree-label {&#xA;            display: inline-block;&#xA;        }&#xA;        .school-label {&#xA;            color: #bdd3de;&#xA;            font-style: italic;&#xA;            font-size: 1.05rem;&#xA;        }&#xA;        .timeline-desc .advisor {&#xA;            margin-left: 50px;&#xA;        }&#xA;        .timeline-desc .thesis {&#xA;            margin-left: 50px;&#xA;        }&#xA;        .career-detail {&#xA;            margin-left: 18px;&#xA;        }&#xA;        .promotion-path {&#xA;            margin-left: 18px;&#xA;        }&#xA;    &lt;/style&gt;&#xA;&lt;/head&gt;&#xA;&lt;body&gt;&#xA;    &lt;div class=&#34;container&#34;&gt;&#xA;        &lt;h1&gt;About&lt;/h1&gt;&#xA;        &lt;div class=&#34;section-title&#34;&gt;&lt;span style=&#34;font-size:1.2em; vertical-align:middle;&#34;&gt;üéì&lt;/span&gt; Education&lt;/div&gt;&#xA;        &lt;div class=&#34;education-list&#34;&gt;&#xA;            &lt;div class=&#34;education-item&#34;&gt;&#xA;                &lt;div class=&#34;education-date&#34;&gt;&#xA;                    &lt;span class=&#34;education-date-year&#34;&gt;2017&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;&#xA;                        &lt;span class=&#34;degree-label&#34;&gt;Ph.D. in Mathematics&lt;/span&gt;&#xA;                        &lt;span class=&#34;school-label&#34;&gt;Seoul National University&lt;/span&gt;&#xA;                    &lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc&#34;&gt;&#xA;                        &lt;span class=&#34;thesis&#34;&gt;&lt;b&gt;Thesis:&lt;/b&gt; Secure Computation via Homomorphic Encryption&lt;/span&gt;&lt;br&gt;&#xA;                        &lt;span class=&#34;advisor&#34;&gt;&lt;b&gt;Advisor:&lt;/b&gt; Prof. Jung Hee Cheon&lt;/span&gt;&#xA;                    &lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;education-item&#34;&gt;&#xA;                &lt;div class=&#34;education-date&#34;&gt;&#xA;                    &lt;span class=&#34;education-date-year&#34;&gt;2013&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;&#xA;                        &lt;span class=&#34;degree-label&#34;&gt;M.S. in Mathematics&lt;/span&gt;&#xA;                        &lt;span class=&#34;school-label&#34;&gt;Seoul National University&lt;/span&gt;&#xA;                    &lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc&#34;&gt;&#xA;                        &lt;span class=&#34;thesis&#34;&gt;&lt;b&gt;Thesis:&lt;/b&gt; Efficient Inversion Algorithms and Its Application to ECDLP&lt;/span&gt;&lt;br&gt;&#xA;                        &lt;span class=&#34;advisor&#34;&gt;&lt;b&gt;Advisor:&lt;/b&gt; Prof. Jung Hee Cheon&lt;/span&gt;&#xA;                    &lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;education-item&#34;&gt;&#xA;                &lt;div class=&#34;education-date&#34;&gt;&#xA;                    &lt;span class=&#34;education-date-year&#34;&gt;2010&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;&#xA;                        &lt;span class=&#34;degree-label&#34;&gt;B.S. in Mathematics&lt;/span&gt;&#xA;                        &lt;span class=&#34;school-label&#34;&gt;KAIST&lt;/span&gt;&#xA;                    &lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;        &lt;/div&gt;&#xA;        &lt;div class=&#34;section-title&#34;&gt;&lt;span style=&#34;font-size:1.2em; vertical-align:middle;&#34;&gt;üíº&lt;/span&gt; Career&lt;/div&gt;&#xA;        &lt;div class=&#34;timeline&#34;&gt;&#xA;            &lt;div class=&#34;timeline-item current&#34;&gt;&#xA;                &lt;div class=&#34;timeline-date&#34;&gt;&#xA;                    &lt;span class=&#34;timeline-date-start&#34;&gt;2025.03&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-dash&#34;&gt;‚Äì&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-end&#34;&gt;&amp;nbsp;&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;Jeonbuk National University&lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc career-detail&#34;&gt;Assistant Professor, Department of Software Engineering&lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;timeline-item&#34;&gt;&#xA;                &lt;div class=&#34;timeline-date&#34;&gt;&#xA;                    &lt;span class=&#34;timeline-date-start&#34;&gt;2022.01&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-dash&#34;&gt;‚Äì&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-end&#34;&gt;2025.02&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;DESILO&lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc career-detail&#34;&gt;Cryptographic Researcher, Research Team&lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;timeline-item&#34;&gt;&#xA;                &lt;div class=&#34;timeline-date&#34;&gt;&#xA;                    &lt;span class=&#34;timeline-date-start&#34;&gt;2020.04&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-dash&#34;&gt;‚Äì&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-end&#34;&gt;2021.11&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;Hanyang University&lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc career-detail&#34;&gt;Postdoctoral Researcher, Department of Mathematics&lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc career-detail&#34;&gt;&lt;b&gt;Advisor:&lt;/b&gt; Prof. Jae Hong Seo&lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;timeline-item&#34;&gt;&#xA;                &lt;div class=&#34;timeline-date&#34;&gt;&#xA;                    &lt;span class=&#34;timeline-date-start&#34;&gt;2018.08&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-dash&#34;&gt;‚Äì&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-end&#34;&gt;2019.03&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;MEDIUM&lt;/div&gt;&#xA;                    &lt;div class=&#34;timeline-desc career-detail&#34;&gt;Researcher&lt;/div&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;            &lt;div class=&#34;timeline-item&#34;&gt;&#xA;                &lt;div class=&#34;timeline-date&#34;&gt;&#xA;                    &lt;span class=&#34;timeline-date-start&#34;&gt;2017.02&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-dash&#34;&gt;‚Äì&lt;/span&gt;&#xA;                    &lt;span class=&#34;timeline-date-end&#34;&gt;2018.07&lt;/span&gt;&#xA;                &lt;/div&gt;&#xA;                &lt;div class=&#34;timeline-content&#34;&gt;&#xA;                    &lt;div class=&#34;timeline-title&#34;&gt;Korea Telecom&lt;/div&gt;&#xA;                    &lt;ul class=&#34;timeline-desc-list promotion-path&#34;&gt;&#xA;                        &lt;li class=&#34;promotion-step&#34;&gt;&#xA;                            &lt;span class=&#34;role&#34;&gt;Manager, Blockchain Biz Center&lt;/span&gt;&#xA;                            &lt;span class=&#34;period&#34;&gt;2018.01 ‚Äì 2018.07&lt;/span&gt;&#xA;                        &lt;/li&gt;&#xA;                        &lt;li class=&#34;promotion-step&#34;&gt;&#xA;                            &lt;span class=&#34;role&#34;&gt;Associate, Blockchain Center&lt;/span&gt;&#xA;                            &lt;span class=&#34;period&#34;&gt;2017.02 ‚Äì 2017.12&lt;/span&gt;&#xA;                        &lt;/li&gt;&#xA;                    &lt;/ul&gt;&#xA;                &lt;/div&gt;&#xA;            &lt;/div&gt;&#xA;        &lt;/div&gt;&#xA;        &lt;a class=&#34;linkedin-btn&#34; href=&#34;https://www.linkedin.com/in/heewonchung/&#34; target=&#34;_blank&#34; rel=&#34;noopener&#34;&gt;&#xA;          &lt;img src=&#34;https://cdn-icons-png.flaticon.com/512/174/174857.png&#34; alt=&#34;LinkedIn&#34; style=&#34;width:32px; height:32px; display:block;&#34;&gt;&#xA;        &lt;/a&gt;&#xA;    &lt;/div&gt;&#xA;&#xA;    &lt;script&gt;&#xA;    // ÌÉÄÏûÑÎùºÏù∏ Ïï†ÎãàÎ©îÏù¥ÏÖò Î∞è ÏÑ† Í∏∏Ïù¥ ÎèôÏ†Å Ï°∞Ï†ï (ÏµúÏÜåÌôî)&#xA;    document.addEventListener(&#39;DOMContentLoaded&#39;, function() {&#xA;      // ÌÉÄÏûÑÎùºÏù∏ Ïï†ÎãàÎ©îÏù¥ÏÖò&#xA;      const items = document.querySelectorAll(&#39;.timeline-item&#39;);&#xA;      const observer = new window.IntersectionObserver((entries) =&gt; {&#xA;        entries.forEach(entry =&gt; {&#xA;          if (entry.isIntersecting) {&#xA;            entry.target.classList.add(&#39;visible&#39;);&#xA;            observer.unobserve(entry.target);&#xA;          }&#xA;        });&#xA;      }, { threshold: 0.1 });&#xA;&#xA;      items.forEach((item, index) =&gt; {&#xA;        item.style.transitionDelay = `${index * 0.1}s`;&#xA;        observer.observe(item);&#xA;      });&#xA;&#xA;      // ÌÉÄÏûÑÎùºÏù∏ ÏÑ† Í∏∏Ïù¥ ÎèôÏ†Å Ï°∞Ï†ï&#xA;      const timeline = document.querySelector(&#39;.timeline&#39;);&#xA;      const timelineItems = timeline.querySelectorAll(&#39;.timeline-item&#39;);&#xA;      if (timelineItems.length &gt; 0) {&#xA;        const first = timelineItems[0];&#xA;        const last = timelineItems[timelineItems.length - 1];&#xA;        const top = first.offsetTop;&#xA;        const bottom = last.offsetTop + last.offsetHeight;&#xA;        timeline.style.setProperty(&#39;--timeline-line-height&#39;, (bottom - top) + &#39;px&#39;);&#xA;      }&#xA;    });&#xA;    &lt;/script&gt;&#xA;&lt;/body&gt;&#xA;&lt;/html&gt;</description>
